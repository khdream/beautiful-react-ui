{
  "require": [ "@babel/register" ],
  "all": true,
  "check-coverage": true,
  "extension": [ ".js" ],
  "include": [ "src/**/*.js" ],
  "exclude": [
    "src/**/index.js",
    "src/**/lazy.js",
    "**/*.spec.js",
    "src/shared/types/**/*.js",
    "src/shared/functions/checkOnAllowedChildren.js"
  ],
  "branches": 90,
  "lines": 90,
  "functions": 90,
  "statements": 90
}
